{
    "env": {
        "browser": true,
        "es2021": true
    },
    "extends": [
        "plugin:react/recommended",
        "airbnb",
        "prettier"
    ],
    "overrides": [
        {"files": ["*.js", "*.jsx"]}
    ],
    "parserOptions": {
        "ecmaVersion": "latest",
        "sourceType": "module"
    },
    "plugins": [
        "react", "prettier"
    ],
    "rules": {
        "react/react-in-jsx-scope": "off",
        "react/prop-types": "off",
        "react/destructuring-assignment": "off",
        "import/prefer-default-export": "off",
        "import/extensions": "off",
        "import/no-cycle": "off"
    }
}
