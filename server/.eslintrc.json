{
  "env": {
    "commonjs": true,
    "es2021": true,
    "node": true
  },
  "extends": ["airbnb-base", "prettier", "plugin:node/recommended"],
  "overrides": [],
  "parserOptions": {
    "ecmaVersion": "latest"
  },
  "plugins": ["prettier"],
  "rules": {
    "import/extensions": "off",
    "func-names": "off",
    "consistent-return": "off",
    "node/no-unsupported-features/es-syntax": "off"
  }
}
